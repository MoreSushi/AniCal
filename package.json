{
  "name": "anical",
  "productName": "AniCal",
  "version": "1.1.0-alpha",
  "description": "Generate ",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "builder-installer": "electron-builder"
  },
  "build": {
    "appId": "anical",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/images/icons/icon.ico"
    },
    "nsis": {
      "installerIcon": "src/images/icons/icon.ico",
      "uninstallerIcon": "src/images/icons/icon.ico",
      "uninstallDisplayName": "AniCal",
      "license": "LICENSE.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [
    "Anilist",
    "Calendar",
    "Schedule"
  ],
  "author": {
    "name": "MoreSushi",
    "email": "onesushi.dev@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "electron-squirrel-startup": "^1.0.1",
    "ical-generator": "^9.0.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "electron-builder": "^25.1.8",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "electron": "37.2.4"
  }
}
